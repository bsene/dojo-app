# Backend 

JWT_SECRET = 'your_secret'

MONGO_USERNAME = 'user_name'

MONGO_PASSWORD = 'password'

MONGO_CLUSTER = 'cluster_name'

MONGO_DBNAME = 'db_name'
  
(in production only: PORT = 5001)

#on dev
MONGO_URL=mongodb://mongo:27017/${MONGO_DBNAME}

#on production
mongodb+srv://${process.env.MONGO_USERNAME}:${process.env.MONGO_PASSWORD}@${process.env.MONGO_CLUSTER}.mongodb.net/${process.env.MONGO_DBNAME}?retryWrites=true&w=majority

# Frontend

(in production only: PUBLIC_URL = 'public_url')


Note: put all those variables in .env on the root level.